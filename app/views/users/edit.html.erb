<%= form_with model: @user, url: users_path, local: true do |f| %>
  <%= f.label "username" %>
  <%= f.text_field 'username', placeholder: 'superUser22' %>
  <%= f.label 'email' %>
  <%= f.email_field 'email', placeholder: 'example@example.com' %>
  <%= f.label 'password' %>
  <%= f.password_field 'password' %>
  <%= f.submit %>
<% end %>
